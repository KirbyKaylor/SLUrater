<% include chunks/head %>
<% include chunks/nav %>
<div class="category">
    <h1>Courses</h1>
    <a id="extLink" href="http://www.stlawu.edu/academics" target="_blank">stlawu.edu/academics</a>
    <br><br>
    <table>
        <tr>
            <th>Name</th>
            <th>Description</th>
        </tr>
        <% course.forEach(function(page) { %>
            <tr>
                <td><a href="/coursespage/<%= page._id %>"><%= page.title %></td>
                <td><%= page.description %></td>
            </tr>
        <% }); %>
    </table>
</div>


